<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
    <title>JW API</title>
    <parser>
        <target>build/documentation</target>
        <visibility>public</visibility>
    </parser>
    <transformer>
        <target>build/documentation</target>
    </transformer>
    <files>
        <directory>src/Jwapi</directory>
        <ignore>src/Jwapi/Traits/*</ignore>
    </files>
    <transformations>
        <template name="responsive-twig"/>
    </transformations>
    <logging>
        <level>warn</level>
        <paths>
            <default>build/logs/docs_{DATE}.log</default>
            <errors>build/logs/docs_{DATE}.errors.log</errors>
        </paths>
    </logging>
</phpdocumentor>